{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:fix": "npm run install-rollup && vitest run",
    "install-rollup": "npm install @rollup/rollup-linux-x64-gnu --save-dev --force || npm install @rollup/rollup-darwin-x64 --save-dev --force || npm install @rollup/rollup-darwin-arm64 --save-dev --force || npm install @rollup/rollup-win32-x64-msvc --save-dev --force || true",
    "lint": "eslint src --ext .js,.vue,.ts",
    "lint:fix": "eslint src --ext .js,.vue,.ts --fix",
    "format": "prettier --write src/**/*.{vue,js,ts,css,json}",
    "format:check": "prettier --check src/**/*.{vue,js,ts,css,json}",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "build:type-check": "vue-tsc --noEmit && vite build",
    "ci": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "ci:fix": "npm run lint:fix && npm run format && npm run type-check"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@sentry/vue": "^9.35.0",
    "@types/marked": "^5.0.2",
    "@vueuse/motion": "^3.0.3",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.6.7",
    "dompurify": "^3.2.6",
    "echarts": "^5.6.0",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^18.0.1",
    "postcss": "^8.5.4",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  }
}
